{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/giveaway-block",
  "version": "0.1.0",
  "title": "Giveaway Block",
  "category": "widgets",
  "icon": "awards",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "accounts": {
      "type": "string",
      "default": {
        "twitter": false,
        "youttube": false
      }
    },
    "tweet": {
      "type": "string",
      "default": {
        "text": "",
        "message": "",
        "url": ""
      }
    }
  },
  "textdomain": "giveaway-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}